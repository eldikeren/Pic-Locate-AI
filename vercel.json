{
  "version": 2,
  "builds": [
    {
      "src": "fastapi_drive_ai_v4_integrated.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/fastapi_drive_ai_v4_integrated.py"
    },
    {
      "src": "/v4/(.*)",
      "dest": "/fastapi_drive_ai_v4_integrated.py"
    },
    {
      "src": "/indexing/(.*)",
      "dest": "/fastapi_drive_ai_v4_integrated.py"
    },
    {
      "src": "/stats/(.*)",
      "dest": "/fastapi_drive_ai_v4_integrated.py"
    },
    {
      "src": "/health",
      "dest": "/fastapi_drive_ai_v4_integrated.py"
    },
    {
      "src": "/docs",
      "dest": "/fastapi_drive_ai_v4_integrated.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_KEY": "@supabase_key",
    "OPENAI_API_KEY": "@openai_api_key",
    "GOOGLE_CLIENT_ID": "@google_client_id",
    "GOOGLE_CLIENT_SECRET": "@google_client_secret"
  },
  "functions": {
    "fastapi_drive_ai_v4_integrated.py": {
      "maxDuration": 300
    }
  }
}